    {
      "version": "1.1.2",
      "uniconv_compat": ">=0.1.0",
      "interface": "native",
      "dependencies": [
        {
          "name": "libavformat-dev",
          "type": "system",
          "check": "pkg-config --exists libavformat",
          "install_hint": "brew install ffmpeg (macOS) or apt install libavformat-dev (Linux)"
