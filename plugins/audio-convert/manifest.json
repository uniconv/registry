{
  "name": "audio-convert",
  "description": "Audio format conversion and audio extraction from video (mp3, wav, flac, aac, ogg, opus, m4a, wma, aiff, alac, ac3)",
  "author": "uniconv",
  "license": "MIT",
  "repository": "https://github.com/uniconv/plugins",
  "keywords": [
    "audio",
    "music",
    "sound",
    "conversion",
    "ffmpeg"
  ],
