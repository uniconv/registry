{
  "name": "video-to-gif",
  "description": "Convert video files to animated GIF using FFmpeg",
  "author": "uniconv",
  "license": "MIT",
  "repository": "https://github.com/uniconv/plugins",
  "keywords": [
    "video",
    "gif",
    "animation",
    "conversion",
    "ffmpeg"
  ],
  "releases": [
    {
      "version": "1.0.0",
      "uniconv_compat": ">=0.1.0",
      "interface": "native",
      "dependencies": [
        {
          "name": "libavformat-dev",
          "type": "system",
          "check": "pkg-config --exists libavformat"
        },
        {
          "name": "libavcodec-dev",
          "type": "system",
          "check": "pkg-config --exists libavcodec"
        },
        {
          "name": "libavutil-dev",
          "type": "system",
          "check": "pkg-config --exists libavutil"
        },
        {
          "name": "libswscale-dev",
          "type": "system",
          "check": "pkg-config --exists libswscale"
        },
        {
          "name": "libavfilter-dev",
          "type": "system",
          "check": "pkg-config --exists libavfilter"
        }
      ],
      "artifact": {
        "linux-x86_64": {
          "url": "https://github.com/uniconv/plugins/releases/download/video-to-gif-v1.0.0/video-to-gif-1.0.0-linux-x86_64.tar.gz",
          "sha256": "4605dcc8bcf1f2bdccfbab4cd849947d17834bf9c5d1376eb94d7f0c1195d6cf"
        }
      }
    }
  ]
}
