{
  "name": "image-convert",
  "description": "Image format conversion using libvips",
  "author": "uniconv",
  "license": "MIT",
  "repository": "https://github.com/uniconv/plugins",
  "keywords": [
    "image",
    "vips",
    "jpg",
    "png",
    "webp",
    "heic",
    "conversion"
  ],
  "releases": [
    {
      "version": "1.0.14",
      "uniconv_compat": ">=0.1.0",
      "interface": "native",
      "dependencies": [
        {
          "name": "libvips",
          "type": "system",
          "check": "pkg-config --exists vips-cpp",
          "install_hint": "brew install vips (macOS) or apt install libvips-dev (Linux)"
        },
        {
          "name": "imagemagick",
          "type": "system",
          "check": "command -v convert",
          "install_hint": "brew install imagemagick (macOS) or apt install imagemagick (Linux)",
          "description": "Required for PDF and BMP format support"
        },
        {
          "name": "imagemagick-pdf-delegate",
          "type": "system",
          "check": "convert -list delegate 2>/dev/null | grep -q pdf",
          "install_hint": "Install ghostscript or poppler, then reinstall imagemagick",
          "description": "PDF delegate required. If missing, install ghostscript/poppler then: brew reinstall imagemagick"
        }
      ],
      "artifact": {
        "linux-x86_64": {
