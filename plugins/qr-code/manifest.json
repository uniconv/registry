{
  "name": "qr-code",
  "description": "Generate and decode QR codes",
  "author": "uniconv",
  "license": "MIT",
  "repository": "https://github.com/uniconv/plugins",
  "keywords": [
    "qr",
    "qr-code",
    "barcode",
    "qrcode",
    "encode",
    "decode"
  ],
  "releases": [
    {
      "version": "1.0.1",
      "uniconv_compat": ">=0.1.0",
      "interface": "cli",
      "dependencies": [
        {
          "name": "python3",
          "type": "system",
          "version": ">=3.8",
          "install_hint": "brew install python3 (macOS) or apt install python3 (Linux)"
        },
        {
          "name": "segno",
          "type": "python",
          "description": "Required for QR code generation",
          "install_hint": "pip install segno"
        },
        {
          "name": "zbar",
          "type": "system",
          "check": "which zbarimg",
          "description": "Required by pyzbar for QR code decoding",
          "install_hint": "brew install zbar (macOS) or apt install libzbar0 (Linux)"
        },
        {
          "name": "Pillow",
          "type": "python",
          "description": "Required for QR code decoding",
          "install_hint": "pip install Pillow"
        },
        {
          "name": "pyzbar",
          "type": "python",
          "description": "Required for QR code decoding",
          "install_hint": "pip install pyzbar"
        }
      ],
